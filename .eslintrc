{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, {"ignorePureComponents": false}],
    "semi": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "jsx-a11y/no-static-element-interactions": ["error", { "handlers": [] } ],
  }
}
